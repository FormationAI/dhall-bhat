    let Transformer = ../Transformer/Type

in  let Monad = ../Monad/Type

in  let ReaderT = ./Type

in    λ(r : Type)
    → (   { lift =
                λ(m : Type → Type)
              → λ(monad : Monad m)
              → λ(a : Type)
              → λ(ma : m a)
              → λ(env : r)
              → ma
          }
        : Transformer (ReaderT r)
      )
