  λ(object : Kind)
→ λ(arrow : object → object → Type)
→ λ(semigroupoid : ./../Semigroupoid/Type object arrow)
→ λ(r : object)
→ λ(m : object → object)
→ λ(a : object)
→ λ(rPrime : object)
→ λ(f : arrow rPrime r)
→ λ(reader : ./Type object arrow r m a)
→ semigroupoid.compose rPrime r (m a) reader f
