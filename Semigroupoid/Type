  λ(object : Kind)
→     let profunctor = object → object → Type
  
  in  ./../Semigroup/Type
      profunctor
      (   λ(p : profunctor)
        → λ(q : profunctor)
        → ∀(a : object) → ∀(b : object) → p a b → q a b
      )
      (   λ(p : profunctor)
        → λ(q : profunctor)
        → λ(a : object)
        → λ(b : object)
        → ∀(r : Type) → (∀(z : object) → p z b → q a z → r) → r
      )
