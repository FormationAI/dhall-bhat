  λ(a : Type)
→ ./../Comonad/fromExtractExtend
  Type
  (./../uncurryT Type Type Type ./../Function/Type)
  ./../Function/category
  (./Type a)
  (λ(b : Type) → λ(tup : ./Type a b) → tup._2)
  (   λ(c : Type)
    → λ(b : Type)
    → λ(f : ./Type a b → c)
    → λ(tup : ./Type a b)
    → { _1 = tup._1, _2 = f tup }
