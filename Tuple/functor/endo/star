    let Tuple = ./../../Type

in    λ(m : Type → Type)
    → λ(functor : ./../../../Functor/Endo/Set m)
    → λ(a : Type)
    →   { map =
              λ(b : Type)
            → λ(c : Type)
            → λ(f : b → m c)
            → λ(t : Tuple a b)
            → functor.map c (Tuple a c) (λ(x : c) → t ⫽ { _2 = x }) (f t._2)
        }
      : ./../../../Functor/Endo/Star/Type
        Type
        (./../../../uncurryT Type Type Type ./../../../Function/Type)
        m
        (Tuple a)
