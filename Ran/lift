  λ(cObject : Kind)
→ λ(cpObject : Kind)
→ λ(cpArrow : cpObject → cpObject → Type)
→ λ(dObject : Kind)
→ λ(dArrow : dObject → dObject → Type)
→ λ(dSemigroupoid : ./../Semigroupoid/Type dObject dArrow)
→ λ(h : cpObject → dObject)
→ λ(functor : ./../Functor/Type cpObject cpArrow dObject dArrow h)
→ λ(f : cObject → cpObject)
→ λ(g : cObject → dObject)
→ λ(join : ∀(b : cObject) → dArrow (h (f b)) (g b))
→ λ(a : cpObject)
→ λ(b : cObject)
→ λ(k : cpArrow a (f b))
→ dSemigroupoid.op
  (h a)
  (g b)
  (   λ(r : Type)
    → λ(arrowsOut : ∀(z : dObject) → dArrow z (g b) → dArrow (h a) z → r)
    → arrowsOut (h (f b)) (join b) (functor.map a (f b) k)
  )
