    let cObject = Type

in  let cpObject = Type

in  let cpArrow = ./../Function/Type

in  let dObject = Type

in    λ(p : cObject → cpObject)
    → λ(applicative : ./../Applicative/Type p)
    → λ(f : cObject → dObject)
    → λ(a : cpObject)
    → λ(ran : ./Type cObject cpObject cpArrow p f a)
    → ran a (applicative.identity a)
