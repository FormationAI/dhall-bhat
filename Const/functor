  λ(cObject : Kind)
→ λ(cArrow : cObject → cObject → Type)
→ λ(cCategory : ./../Category/Type cObject cArrow)
→ λ(dObject : Kind)
→ λ(dArrow : dObject → dObject → Type)
→ λ(a : cObject)
→   { map =
          λ(b : dObject)
        → λ(c : dObject)
        → λ(_ : dArrow b c)
        → cCategory.identity a
    }
  : ./../Functor/Type dObject cObject dArrow cArrow (./Type cObject dObject a)
