    let Id = ./Type 

in  let Monad = ../Monad/Type 

in  (   { map =
            ./Functor .map
        , pure =
            ./Applicative .pure
        , ap =
            ./Applicative .ap
        , bind =
            λ(a : Type) → λ(b : Type) → λ(fa : Id a) → λ(k : a → Id b) → k fa
        }
      : Monad Id
    )
