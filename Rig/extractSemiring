    let object = Type

in  let cat = ./../Category/Monoidal/Set/cartesian

in    λ(m : object)
    → λ(rig : ./Type m)
    →   { additive =
            ./../Monoid/Commutative/extractSemigroup m rig.additive
        , multiplicative =
            rig.multiplicative
        }
      : ./../Semiring/Type object cat m
