  λ(w : Type → Type)
→ λ(comonad : ./../Comonad/Type w)
→   { id =
        comonad.extract
    , compose =
          λ(a : Type)
        → λ(b : Type)
        → λ(c : Type)
        → λ(g : w b → c)
        → λ(f : w a → b)
        → λ(x : w a)
        → g (comonad.extend a b f x)
    }
  : ./../Category/type (./type w)
