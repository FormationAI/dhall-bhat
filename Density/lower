    let object = Type

in  let arrow = ./../Function/Type

in    λ(f : object → object)
    → λ(comonad : ./../Comonad/Type object arrow f)
    → ./../Lan/lower
      object
      object
      arrow
      f
      (./../Comonad/extractEndofunctor object arrow f comonad)
      f
      f
      comonad.op
