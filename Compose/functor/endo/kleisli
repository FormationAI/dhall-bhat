  λ(m : Type → Type)
→     let MFunctor = ./../../../Functor/Endo/Kleisli m
  
  in    λ(f : Type → Type)
      → λ(fKleisli : MFunctor f)
      → λ(g : Type → Type)
      → λ(gKleisli : MFunctor g)
      →   { map =
                λ(a : Type)
              → λ(b : Type)
              → λ(fn : a → m b)
              → fKleisli.map (g a) (g b) (gKleisli.map a b fn)
          }
        : MFunctor (./../../type f g)
