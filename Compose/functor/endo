  λ(object : Kind)
→ λ(arrow : object → object → Type)
→     let Endofunctor = ./../../Functor/Endo/Type object arrow
  
  in    λ(f : object → object)
      → λ(g : object → object)
      → λ(fFunctor : Endofunctor f)
      → λ(gFunctor : Endofunctor g)
      →   { map =
                λ(a : object)
              → λ(b : object)
              → λ(k : arrow a b)
              → fFunctor.map (g a) (g b) (gFunctor.map a b k)
          }
        : Endofunctor (./../Type object f g)
