    let Endofunctor = ./../../../Functor/Endo/Types

in    λ(f : Type → Type)
    → λ(g : Type → Type)
    → λ(fFunctor : Endofunctor f)
    → λ(gFunctor : Endofunctor g)
    →   { map =
              λ(a : Type)
            → λ(b : Type)
            → λ(k : a → b)
            → fFunctor.map (g a) (g b) (gFunctor.map a b k)
        }
      : Endofunctor (./../../Type f g)
