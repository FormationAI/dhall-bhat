    let Foldable = ./Type

in  let Monoid = ./../Monoid/Set

in    λ(m : Type)
    → λ(monoid : Monoid m)
    → λ(t : Type → Type)
    → λ(foldable : Foldable t)
    → λ(a : Type)
    → λ(f : a → m)
    → foldable.fold
      a
      m
      (λ(y : m) → λ(x : a) → monoid.op { _1 = f x, _2 = y })
      (monoid.identity {=})
