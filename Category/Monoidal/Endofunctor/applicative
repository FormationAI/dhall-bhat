    let kArrow = ./../../../Function/Kind

in  let kProduct = ./../../../Tuple/Kind

in  let object = Type

in    λ(cat : ./../../Monoidal/Type kArrow kProduct object)
    → ./../../Duoidal/extractAdditive
      kArrow
      kProduct
      (kArrow object object)
      (./../../Duoidal/Endofunctor cat)
