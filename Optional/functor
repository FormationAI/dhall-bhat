let Optional/fold =
      https://prelude.dhall-lang.org/Optional/fold.dhall
        sha256:c5b9d72f6f62bdaa0e196ac1c742cc175cd67a717b880fb8aec1333a5a4132cf

in    { map =
          λ(a : Type) →
          λ(b : Type) →
          λ(f : a → b) →
          λ(o : Optional a) →
            Optional/fold a o (Optional b) (λ(x : a) → Some (f x)) (None b)
      }
    : ../Functor/Type Optional
