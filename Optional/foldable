let Foldable = ../Foldable/Type

let Optional/fold =
      https://prelude.dhall-lang.org/Optional/fold.dhall
        sha256:c5b9d72f6f62bdaa0e196ac1c742cc175cd67a717b880fb8aec1333a5a4132cf

in    { fold =
          λ(a : Type) →
          λ(ts : Optional a) →
          λ(b : Type) →
          λ(f : a → b → b) →
          λ(z : b) →
            Optional/fold a ts b (λ(x : a) → f x z) z
      }
    : Foldable Optional
