    let Equality = ./../Eq/Type

in    λ(a : Type)
    → λ(eq : Equality a)
    →   { eq =
              λ(x : Optional a)
            → λ(y : Optional a)
            → Optional/fold
              a
              x
              Bool
              (λ(xx : a) → Optional/fold a y Bool (eq.eq xx) False)
              (Optional/fold a y Bool (λ(yy : a) → False) True)
        }
      : Equality (Optional a)
