    let Endofunctor = ./../../../Functor/Endo/Types

in  let EitherT = ./../../Type

in  let Either = ./../../../Either/Type

in  let eitherF = ./../../../Either/functor/endo/types

in  let composeF = ./../../../Compose/functor/endo/types

in    λ(a : Type)
    → λ(f : Type → Type)
    → λ(functor : Endofunctor f)
    → composeF f (Either a) functor (eitherF a) : Endofunctor (EitherT a f)
