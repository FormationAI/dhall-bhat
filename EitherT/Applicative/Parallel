    let Applicative = ./../../Applicative/Type

in  let Semigroup =
          ./../../Semigroup/Type Type ./../../Function/Type ./../../Tuple/Type

in    λ(a : Type)
    → λ(semi : Semigroup a)
    → λ(m : Type → Type)
    → λ(applicative : Applicative m)
    →   ./../../Compose/Applicative
        m
        (./../../Either/Type a)
        applicative
        (./../../Either/Applicative/Parallel a semi)
      : Applicative (./../Type a m)
