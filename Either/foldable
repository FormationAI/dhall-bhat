    let Foldable = ./../Foldable/Type

in  let Either = ./Type

in    λ(a : Type)
    →   { fold =
              λ(b : Type)
            → λ(either : Either a b)
            → λ(c : Type)
            → λ(f : b → c → c)
            → λ(z : c)
            → ./fold a b c (λ(l : a) → z) (λ(r : b) → f r z) either
        }
      : Foldable (Either a)
