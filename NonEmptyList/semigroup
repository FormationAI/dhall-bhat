    let NonEmpty = ./Type

in    λ(a : Type)
    →     let List/op = (./../List/semigroup a).op
      
      in    { op =
                  λ(p : { _1 : NonEmpty a, _2 : NonEmpty a })
                → { head =
                      p._1.head
                  , tail =
                      List/op { _1 = p._1.tail, _2 = ./toList a p._2 }
                  }
            }
          : ./../Semigroup/Set (NonEmpty a)
