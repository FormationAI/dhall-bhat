    let object = Type

in    λ ( m
        : object
        )
    → λ(ring : ./Type m)
    →   { additive =
            ./../../Group/OrderedCommutative/extractPartiallyOrderedCommutativeGroup
            m
            ring.additive
        , multiplicative =
            ring.multiplicative
