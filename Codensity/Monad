  λ(m : Type → Type)
→ ./../Monad/fromPureBind
  Type
  ./../Function/Type
  ./../Function/category
  (./Type m)
  (λ(a : Type) → λ(x : a) → λ(b : Type) → λ(k : a → m b) → k x)
  (   λ(a : Type)
    → λ(b : Type)
    → λ(k : a → ./Type m b)
    → λ(codensity : ./Type m a)
    → λ(c : Type)
    → λ(l : b → m c)
    → codensity c (λ(x : a) → k x c l)
  )
