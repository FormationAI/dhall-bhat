  λ(object : Kind)
→ λ(arrow : object → object → Type)
→ λ(category : ./../Category/Type object arrow)
→ λ(f : object → object)
→ λ(pure : ∀(a : object) → arrow a (f a))
→ λ(bind : ∀(a : object) → ∀(b : object) → arrow a (f b) → arrow (f a) (f b))
→   { identity =
        pure
    , op =
        λ(a : object) → bind (f a) a (category.identity (f a))
    }
  : ./Type object arrow f
