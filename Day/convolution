  λ(cObject : Kind)
→ λ(c : ./../Category/Monoidal/Type cObject)
→     let vObject = Type
  
  in    λ(v : ./../Category/Monoidal/Type vObject)
      → λ(f : cObject → vObject)
      → λ(g : cObject → vObject)
      → λ(a : cObject)
      → λ(r : vObject)
      →   ∀(b : { _1 : cObject, _2 : cObject })
        → c.arrow (c.op b._1 b._2) a
        → v.arrow (v.op (f b._1) (g b._2)) r
