    { first =
        λ(a : Type) →
        λ(b : Type) →
        λ(c : Type) →
        λ(f : a → b) →
        λ(p : { _1 : a, _2 : c }) →
          { _1 = f p._1, _2 = p._2 }
    , second =
        λ(a : Type) →
        λ(b : Type) →
        λ(c : Type) →
        λ(f : b → c) →
        λ(p : { _1 : a, _2 : b }) →
          { _1 = p._1, _2 = f p._2 }
    }
  ∧ ./profunctor
: ../Strong/Type ./Type
