    let kArrow = ./../Function/Kind

in  let kProduct = ./../Tuple/Kind

in    λ(object : Kind)
    → λ(cat : ./../Category/Monoidal/Type kArrow kProduct object)
    → λ(m : object)
    → { monoid :
          ./../Monoid/Type object cat m
      , comonod :
          ./../Comonoid/Type object cat m
      }
