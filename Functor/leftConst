    let Functor = ./Type

in  let leftConst
        :   ∀(f : Type → Type)
          → ∀(functor : Functor  f)
          → ∀(a : Type)
          → ∀(b : Type)
          → a
          → f b
          → f a
        =   λ(f : Type → Type)
          → λ(functor : Functor  f)
          → λ(a : Type)
          → λ(b : Type)
          → λ(val : a)
          → λ(fb : f b)
          → functor.map b a (λ(bVal : b) → val) fb

in  leftConst
